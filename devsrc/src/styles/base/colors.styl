.grand-oasis-cancun
	.booking
		.btn-reserva-booking
			background  $gocBkg
		.title--booking
			color $gocBkg
		&__home
			+from-width(5)
				.booking-container
					background url('../img/booking/goc-book.png') center no-repeat			
	.principal
		background $gocBkg
		+from-width(5)
			background none
		.logo_field
			.icon
				color $gocColor
		.lang_d
			> li.language
				.submenu li
					color $gocColor
					&.active
					&:hover
						background $gocBkg
	.title--section
		color $gocColor
		&:after
			border-bottom 1px solid $gocColor
	.newsletter
		.btn-newsletter
			background  $gocBkg
	.card--home--room
		.card--topimage--item
			&__image--buttons
				a
					color $gocBkg
					border 1px solid $gocBkg
					box-shadow none
					+from-width(4)
						color white
						border 1px solid white
						box-shadow 0 2px 14px 0 rgba(0,0,0,0.1)
			&__info--button
				a.btn-reserva
					background $gocBkg
	.tabs-image--item-container
		&:after
			background rgba($gocHov, 0.45)
	.tabs-image--item-content
		a.btn
			&:hover
				color $gocBkg
	.grid-gallery--item-images--item
		a:hover
			+from-width(5)
				&:after
					background rgba($gocHov, 0.65)
	.footer--redes-sociales
		color $gocBkg
		
				